    <!--[if IE]>
    <script type="text/javascript" src="assets/plugins/jquery-knob/excanvas.js"></script>
    <![endif]-->
    <script src="~/vert/assets/plugins/jquery-knob/jquery.knob.js"></script>

    <!--Morris Chart-->
    <script src="~/vert/assets/plugins/morris/morris.min.js"></script>
    <script src="~/vert/assets/plugins/raphael/raphael-min.js"></script>

    <!-- Dashboard init -->
    <script src="~/vert/assets/pages/jquery.dashboard.js"></script>

    <!-- App js -->
    <script src="~/vert/assets/js/jquery.core.js"></script>
    <script src="~/vert/assets/js/jquery.app.js"></script>

    <script src="~/Scripts/js/site.js"></script>

    @RenderSection("Scripts", required: false)
